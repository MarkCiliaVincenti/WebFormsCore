<Project>

    <PropertyGroup>
        <Version>0.0.1-alpha.32</Version>
        <PackageVersion>$(Version)</PackageVersion>
        <Authors>WebFormsCore</Authors>
        <RepositoryUrl>https://github.com/WebFormsCore/WebFormsCore</RepositoryUrl>
        <Copyright>Copyright Â© 2023 WebFormsCore</Copyright>
        <PackageTags>webformscore</PackageTags>
        <PackageProjectUrl>https://github.com/WebFormsCore/WebFormsCore</PackageProjectUrl>
        <PackageLicense>https://github.com/WebFormsCore/WebFormsCore/blob/main/LICENSE</PackageLicense>
        <NoWarn>$(NoWarn);CS0436;CS9057;NU5128;CS1620</NoWarn>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    </PropertyGroup>

    <PropertyGroup>
        <DotNetVersions>net6.0;net7.0;net8.0</DotNetVersions>
        <ExtensionsVersion>6.0.0</ExtensionsVersion>
        <HttpStackVersion>0.0.1-alpha.12</HttpStackVersion>
    </PropertyGroup>

    <PropertyGroup Condition="$(TargetFramework) == 'net7.0'">
        <ExtensionsVersion>7.0.0</ExtensionsVersion>
    </PropertyGroup>

    <PropertyGroup Condition="$(TargetFramework) == 'net8.0'">
        <ExtensionsVersion>8.0.0-preview.7.23375.6</ExtensionsVersion>
    </PropertyGroup>

</Project>
